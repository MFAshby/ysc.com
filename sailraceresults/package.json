{
  "name": "sailraceresults",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.9.7"
  },
  "scripts": {
    "start": "REACT_APP_API_SERVER=http://localhost:8080/api parcel src/index.html -p 3000",
    "build": "parcel build src/index.html src/index.js --out-dir build --public-url $PUBLIC_URL"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
